{
  "extends": "@adonisjs/tsconfig/tsconfig.app.json",
  "compilerOptions": {
    "rootDir": "./",
    "outDir": "./build",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "#start/*": ["./start/*.js"],
      "#config/*": ["./config/*.js"],
      "#core/*": ["./app/core/*.js"],
      "#common/*": ["./app/common/*.js"],
      "#auth/*": [
        "./app/auth/*.js"
      ],
      "#users/*": ["./app/users/*.js"],
      "#marketing/*": [
        "./app/marketing/*.js"
      ],
      "#home/*": [
        "./app/home/*.js"
      ],
      "#dashboard/*": [
        "./app/dashboard/*.js"
      ],
      "#providers/*": [
        "./providers/*.js"
      ],
      "#tests/*": [
        "./tests/*.js"
      ],
      "#ui/*": [
        "./packages/ui/src/*.js"
      ],
      "#packages/*": [
        "./packages/*.js"
      ]
    }
  },
  "include": [
    "app/**/*.ts",
    "app/**/*.tsx"
  ],
  "exclude": [
    "node_modules",
    "build"
  ]
}
